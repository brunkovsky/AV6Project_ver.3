<?xml version="1.0" encoding="UTF-8" ?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         id="WebApp_ID"
         version="3.0">
    <welcome-file-list>
        <welcome-file>IndexServlet</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>Controller.IndexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>Controller.FileUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReadDataServlet</servlet-name>
        <servlet-class>Controller.ReadDataServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileListReaderServlet</servlet-name>
        <servlet-class>Controller.FileListReaderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModelWorkerServlet</servlet-name>
        <servlet-class>Controller.ModelWorkerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploadDeleteServlet</servlet-name>
        <servlet-class>Controller.FileUploadDeleteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ToDBServlet</servlet-name>
        <servlet-class>Controller.ToDBServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/IndexServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/UploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReadDataServlet</servlet-name>
        <url-pattern>/ReadDataServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileListReaderServlet</servlet-name>
        <url-pattern>/FileListReaderServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModelWorkerServlet</servlet-name>
        <url-pattern>/ModelWorkerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadDeleteServlet</servlet-name>
        <url-pattern>/FileUploadDeleteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ToDBServlet</servlet-name>
        <url-pattern>/ToDBServlet</url-pattern>
    </servlet-mapping>
</web-app>